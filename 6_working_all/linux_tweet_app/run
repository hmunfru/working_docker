## create a user in dockerhub

export DOCKERID=hmunfru
echo $DOCKERID
docker image build --tag $DOCKERID/linux_tweet_app:1.0 .
docker container run --detach --publish 80:80 --name linux_tweet_app $DOCKERID/linux _tweet_app:1.0

docker container run --detach --publish 80:80 --name linux_tweet_app --volume $(pwd):/usr/share/ngin x/html $DOCKERID/linux_tweet_app:1.0


